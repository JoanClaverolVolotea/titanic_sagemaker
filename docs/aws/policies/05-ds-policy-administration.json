{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadOwnIamUserState",
      "Effect": "Allow",
      "Action": [
        "iam:GetUser",
        "iam:ListAttachedUserPolicies"
      ],
      "Resource": "arn:aws:iam::939122281183:user/data-science-user"
    },
    {
      "Sid": "AttachDetachProjectPoliciesOnOwnUser",
      "Effect": "Allow",
      "Action": [
        "iam:AttachUserPolicy",
        "iam:DetachUserPolicy"
      ],
      "Resource": [
        "arn:aws:iam::939122281183:user/data-science-user",
        "arn:aws:iam::939122281183:policy/DataScienceAssumeEnvironmentRoles",
        "arn:aws:iam::939122281183:policy/DataScienceObservabilityReadOnly",
        "arn:aws:iam::939122281183:policy/DataSciencePassroleRestricted",
        "arn:aws:iam::939122281183:policy/DataSciences3DataAccess",
        "arn:aws:iam::939122281183:policy/DataScienceS3TutorialBucketBootstrap",
        "arn:aws:iam::939122281183:policy/DataSciencePolicyAdministration"
      ]
    },
    {
      "Sid": "ManageProjectPolicyVersions",
      "Effect": "Allow",
      "Action": [
        "iam:GetPolicy",
        "iam:CreatePolicyVersion",
        "iam:ListPolicyVersions",
        "iam:DeletePolicyVersion"
      ],
      "Resource": [
        "arn:aws:iam::939122281183:policy/DataScienceAssumeEnvironmentRoles",
        "arn:aws:iam::939122281183:policy/DataScienceObservabilityReadOnly",
        "arn:aws:iam::939122281183:policy/DataSciencePassroleRestricted",
        "arn:aws:iam::939122281183:policy/DataSciences3DataAccess",
        "arn:aws:iam::939122281183:policy/DataScienceS3TutorialBucketBootstrap",
        "arn:aws:iam::939122281183:policy/DataSciencePolicyAdministration"
      ]
    },
    {
      "Sid": "CreateProjectPolicies",
      "Effect": "Allow",
      "Action": [
        "iam:CreatePolicy"
      ],
      "Resource": "*"
    }
  ]
}
